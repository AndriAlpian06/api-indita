generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id        Int      @id @default(autoincrement())
  name      String   @db.VarChar(255)
  email     String   @unique(map: "email") @db.VarChar(255)
  address   String?  @db.VarChar(255)
  password  String?  @db.VarChar(255)
  createdAt DateTime @default(now()) @db.Timestamp(0)
  updateAt  DateTime @default(now()) @db.Timestamp(0)

  @@map("users")
}

model kategori_keyword {
  id        Int      @id @default(autoincrement())
  kategori  String   @db.VarChar(255)
  createdAt DateTime @default(now()) @db.Timestamp(0)
  updateAt  DateTime @default(now()) @db.Timestamp(0)
}

model keyword {
  id          Int      @id @default(autoincrement())
  id_kategori String   @db.VarChar(255)
  keyword     String   @db.VarChar(255)
  createdAt   DateTime @default(now()) @db.Timestamp(0)
  updateAt    DateTime @default(now()) @db.Timestamp(0)
}

model intent {
  id               Int      @id @default(autoincrement())
  nama_intent      String   @db.VarChar(255)
  deskripsi_intent String   @db.VarChar(255)
  id_kategori      String   @db.VarChar(255)
  createdAt        DateTime @default(now()) @db.Timestamp(0)
  updateAt         DateTime @default(now()) @db.Timestamp(0)
}
